{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/set/:code/:slug*",
      "destination": "/index.html"
    },
    {
      "source": "/s/:code",
      "destination": "/index.html"
    },
    {
      "source": "/auth/callback",
      "destination": "/index.html"
    },
    {
      "source": "/privacy",
      "destination": "/index.html"
    },
    {
      "source": "/terms",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api/|_next/|static/|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}